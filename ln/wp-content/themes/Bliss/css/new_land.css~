 max-width:350px;
padding:15px 15px 0;
 position:relative;
 width:100%;
 overflow: hidden;

}


.hari-sticky {
 background:#FFFFFF;
 border-bottom-left-radius:4px;
 border-bottom-right-radius:4px;
 border-top-left-radius:4px;
 border-top-right-radius:4px;
 box-shadow:rgba(0, 0, 0, 0.2) 0 2px 2px, rgba(0, 0, 0, 0.2) 0 1px 5px;
 color:#7E7975;
 margin:0 auto 12px;
 max-width:350px;
 padding:15px;
 position:relative;
 width:100%;
 overflow: hidden;
}

